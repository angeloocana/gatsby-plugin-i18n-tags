{
  "name": "gatsby-plugin-i18n-tags",
  "version": "0.0.3",
  "description": "Multi language feature for Gatsby",
  "homepage": "https://github.com/angeloocana/gatsby-plugin-i18n-tags#readme",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --fix ./src",
    "prebuild": "yarn run lint",
    "build": "babel src --out-dir .",
    "watch": "babel -w src --out-dir .",
    "pretest": "yarn run build",
    "test": "jest && codecov"
  },
  "respository": {
    "type": "git",
    "url": "https://github.com/angeloocana/gatsby-plugin-i18n-tags"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "i18n"
  ],
  "author": "angeloocana.com",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "graphql": "^0.11.1",
    "ptz-i18n": "^0.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "codecov": "^2.3.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.0.2",
    "ptz-assert": "^1.6.8"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "readme": "README.md",
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
